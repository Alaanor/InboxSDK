{
   "background": {
      "scripts": [ "jquery.js", "contentApi.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html",
      "default_title": "Insert DocSend link"
   },
   "content_scripts": [ {
      "js": [ "inboxsdk.js", "ajaxer.js", "contentApi.js", "content.js", "jquery.js", "in-parser.js" ],
      "matches": [ "https://mail.google.com/*", "https://inbox.google.com/*", "http://*.linkedin.com/*", "https://*.linkedin.com/*" ]
   } ],
   "content_security_policy": "script-src 'self' https://*.docsend.com https://*.google.com https://ssl.google-analytics.com https://d2qvtfnm75xrxf.cloudfront.net  https://cdn.segment.com https://*.kissmetrics.com https://api.intercom.io https://js.intercomcdn.com https://widget.intercom.io https://static.intercomcdn.com https://doug1izaerwt3.cloudfront.net https://plus.google.com https://www.google-analytics.com;  object-src 'self';",
   "description": "Quickly create links to any of your DocSend documents right inside Gmail.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "DocSend Extension for Chrome",
   "permissions": [ "storage", "tabs", "clipboardWrite", "webRequest", "webRequestBlocking", "*://d2qvtfnm75xrxf.cloudfront.net/*", "*://*.linkedin.com/*", "https://api.intercom.io/*", "*://docsend.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.4",
   "web_accessible_resources": [ "images/*.*", "fonts/*.*", "blank.png", "content.html" ]
}
