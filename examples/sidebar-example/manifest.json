{
  "name": "Sidebar Example Extension",
  "description": "Sidebar example!",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["inboxsdk.js","bacon.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "web_accessible_resources": [
    "inboxsdk.js.map",
    "monkey.png",
    "monkey-face.jpg",
    "lion.png"
  ],

  "manifest_version": 2
}
